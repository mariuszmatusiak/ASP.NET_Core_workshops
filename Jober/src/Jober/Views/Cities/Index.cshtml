@model IEnumerable<Jober.Models.City>
@{
    ViewData["Title"] = "Cities";
}
<div class="row">
 
    <div class="col-md-9">     

        <table class="table table-bordered" >
            <tr>                
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>                  
                    <td>@item.Name</td>
                </tr>

             }
        </table>
    </div>
</div>